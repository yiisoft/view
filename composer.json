{
    "name": "yiisoft/view",
    "type": "library",
    "description": "Yii View Rendering Library",
    "keywords": [
        "yii",
        "view"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/view",
        "issues": "https://github.com/yiisoft/view/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "psr/container": "1.0.0",
        "psr/container-implementation": "1.0.0",
        "psr/event-dispatcher": "1.0.0",
        "psr/event-dispatcher-implementation": "1.0.0",
        "psr/log": "^1.1",
        "yiisoft/aliases": "^3.0@dev",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/files": "^3.0@dev",
        "yiisoft/html": "^3.0@dev",
        "yiisoft/i18n": "^3.0@dev",
        "yiisoft/json": "^3.0@dev",
        "yiisoft/var-dumper": "^3.0@dev"
    },
    "require-dev": {
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "phpunit/phpunit": "^8.4",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "tests": "config/tests.php"
        }
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\View\\": "src/View",
            "Yiisoft\\Widget\\": "src/Widget",
            "Yiisoft\\Asset\\": "src/Asset"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Tests\\": "tests",
            "Yiisoft\\View\\Tests\\": "tests/View",
            "Yiisoft\\Widget\\Tests\\": "tests/Widget",
            "Yiisoft\\Asset\\Tests\\": "tests/Asset"
        }
    },
    "config": {
        "sort-packages": true
    }
}
